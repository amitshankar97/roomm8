    /user/{id}/favorite-properties
        Method :
            - GET
        Attributes :
            -
        Description :
            - Returns the user's favorite properties list

    /user/{id}/favorite-properties
        Method :
            - POST
        Attributes :
            - favorited user's ID
                - Required : True
        Description :
            - Add a property to the user's favorites list

    /user/{id}/favorite-properties
        Method :
            - DELETE
        Attributes :
            - favorited property's ID
                - Required : True
        Description :
            - Delete a property from the user's favorites list
